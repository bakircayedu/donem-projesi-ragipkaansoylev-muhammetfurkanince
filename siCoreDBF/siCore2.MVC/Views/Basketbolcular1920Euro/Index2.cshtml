@model IEnumerable<siCoreDBF3.DAL.Model.Basketbolcular1920Euro>

@{
    ViewData["Title"] = "Index";
}

<h3 class="tableName">2019/2020 ASÝST ORTALAMALARI</h3>
<div class="table-wrapper">
    <table class="fl-table">
        <thead>
            <tr>
                <th>Basketbolcu</th>
                <th>Takým</th>
                <th>Asist Sayýsý</th>
                

            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FutbolcuIsim)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Takim.TakimAdi)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.YapilanAsistOrtalamasi)
                </td>

                
            </tr>
        }
        </tbody>
    </table>
</div>